{
  "name": "tasty-js",
  "title": "Tasty",
  "version": "0.0.2",
  "description": "Integration testing helper for web",
  "main": "src/main.js",
  "preferGlobal": true,
  "bin": {
    "tasty": "./src/cli.js"
  },
  "dependencies": {
    "glob": "^7.1.0",
    "requireg": "^0.1.6",
    "socket.io": "^1.4.8",
    "tasty-sandbox": "^2.0.4"
  },
  "devDependencies": {
    "jasmine": "^2.5.2",
    "mocha": "^3.1.0"
  },
  "scripts": {
    "test": "node test/run.js",
    "selftest": "printf 'Navigate browser to http://localhost:5678/test.html\\n\\n' && node ./src/cli.js --runner=mocha --include=test/self/mocha/*.js --static-root=test/res && printf '\\nNavigate browser to http://localhost:5678/test.html\\n\\n' && node ./src/cli.js --runner=jasmine --include=test/self/jasmine/*.js --static-root=test/res"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chge/tasty.git"
  },
  "keywords": [
    "integration",
    "testing",
    "web"
  ],
  "author": "chge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chge/tasty/issues"
  },
  "homepage": "https://github.com/chge/tasty"
}
